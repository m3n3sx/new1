<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Live Admin Styler v2.0 - PHP API Documentation</title>
    <parser>
        <target>docs/php</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
        <visibility>public,protected,private</visibility>
    </parser>
    <transformer>
        <target>docs/php</target>
    </transformer>
    <transformations>
        <template name="responsive-twig" />
    </transformations>
    <files>
        <directory>includes</directory>
        <ignore>node_modules/*</ignore>
        <ignore>tests/*</ignore>
        <ignore>vendor/*</ignore>
    </files>
</phpdoc>