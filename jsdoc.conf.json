{
    "source": {
        "include": [
            "./assets/js/",
            "./README.md"
        ],
        "includePattern": "\\.(js|jsx)$",
        "exclude": [
            "./node_modules/",
            "./tests/",
            "./assets/js/*.backup*"
        ]
    },
    "opts": {
        "destination": "./docs/js/",
        "recurse": true,
        "readme": "./README.md"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "metadata": {
        "title": "Live Admin Styler v2.0 - JavaScript API Documentation",
        "version": "2.0.0"
    }
}